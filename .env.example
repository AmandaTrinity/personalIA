# Configurações da aplicação PersonalIA
# IMPORTANTE: Copie este arquivo para .env e configure suas chaves!

# API do Google Gemini
GEMINI_API_KEY=sua_chave_gemini_aqui

# Configurações do Banco de Dados
MONGO_URI=mongodb+srv://usuario:senha@cluster.mongodb.net/?retryWrites=true&w=majority&appName=app
DATABASE_NAME=personalai_db

# Configurações de Segurança
SECRET_KEY=sua_chave_secreta_aqui

# Configurações do Servidor
PORT=8000
HOST=127.0.0.1
DEBUG=False

# Configurações de Logs
LOG_LEVEL=INFO